                                <xs:maxLength value="20" />
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
					
					<xs:element name="dataProvvedimento" type="date" minOccurs="0" />
					
					<xs:element name="desEnteCompetente" minOccurs="0">
                        <xs:simpleType>
                            <xs:restriction base="xs:string">
                                <xs:maxLength value="200" />
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
					
					<xs:element name="idAttivita" type="xs:decimal" minOccurs="1" />
					
                </sequence>
            </extension>
        </complexContent>
    </complexType>
	
	<complexType name="SottoAttivitaEmissivaType">
        <complexContent>
            <extension base="gml:AbstractGMLType">
                <sequence>
				
				    <xs:element name="idAutorizzazione" minOccurs="1">
                        <xs:simpleType>
                            <xs:restriction base="xs:string">
                                <xs:maxLength value="20" />
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
				    
					<xs:element name="desSottoAttivitaEmissiva" minOccurs="1">
                        <xs:simpleType>
                            <xs:restriction base="xs:string">
                                <xs:maxLength value="500" />
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
					
                </sequence>
            </extension>
        </complexContent>
    </complexType>
	
	<complexType name="PuntoEmissioneType">
        <complexContent>
            <extension base="gml:AbstractGMLType">
                <sequence>
				    
					<xs:element name="idPuntoEmissione" type="xs:decimal" minOccurs="1" />
					
					<xs:element name="idAutorizzazione" minOccurs="1">
                        <xs:simpleType>
                            <xs:restriction base="xs:string">
                                <xs:maxLength value="20" />
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
					
					<xs:element name="sigla" minOccurs="1">
                        <xs:simpleType>
                            <xs:restriction base="xs:string">
                                <xs:maxLength value="50" />
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
					
					<xs:element name="desTipoEmissione" minOccurs="0">
                        <xs:simpleType>
                            <xs:restriction base="xs:string">
                                <xs:maxLength value="500" />
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
					
					<xs:element name="portataAeriforme_Nm3_h" type="xs:decimal" minOccurs="1" />
					
					<xs:element name="durataEmissione_h_g" type="xs:decimal" minOccurs="0" />
					
					<xs:element name="frequenzaEmissione" minOccurs="0">
                        <xs:simpleType>
                            <xs:restriction base="xs:string">
                                <xs:maxLength value="150" />
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
					
					<xs:element name="desCostanteDiscontinua" minOccurs="0">
                        <xs:simpleType>
                            <xs:restriction base="xs:string">
                                <xs:maxLength value="50" />
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
					
					<xs:element name="temperaturaEmissione" type="xs:decimal" minOccurs="0" />
					
					<xs:element name="temperaturaAtmosferica" type="xs:decimal" minOccurs="0" />
					
					<xs:element name="indicazioneSistemaAbbattimento" minOccurs="0">
                        <xs:simpleType>
                            <xs:restriction base="xs:string">
                                <xs:maxLength value="500" />
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
					
					<xs:element name="note" minOccurs="0">
                        <xs:simpleType>
                            <xs:restriction base="xs:string">
                                <xs:maxLength value="500" />
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
					
					<!-- qui ci vuole un join -->
                    <xs:element name="provenienzaEmissione" type="siraemissioni:ProvenienzaEmissionePropertyType" minOccurs="1" maxOccurs="unbounded" />
				
					<!-- qui NON ci vuole un join retype 1 vista su 2 oggetti-->
                    <xs:element name="camino" type="siraemissioni:CaminoPropertyType" minOccurs="1" maxOccurs="unbounded" />
					
					<!-- qui ci vuole un join -->
                    <xs:element name="impiantoDiAbbattimento" type="siraemissioni:ImpiantoDiAbbattimentoPropertyType" minOccurs="0" maxOccurs="unbounded" />
					
					<!-- qui ci vuole un join -->
                    <xs:element name="inquinante" type="siraemissioni:InquinantePropertyType" minOccurs="0" maxOccurs="unbounded" />
					
					<!-- qui ci vuole un join -->
                    <xs:element name="impiantoDiCombustione" type="siraemissioni:ImpiantoCombustionePropertyType" minOccurs="0" maxOccurs="unbounded" />
					
                </sequence>
            </extension>
        </complexContent>
    </complexType>
	
	<complexType name="ImpiantoCombustioneType">
        <complexContent>
            <extension base="gml:AbstractGMLType">
                <sequence>
				
				    <xs:element name="idImpiantoCombustione" minOccurs="1">
                        <xs:simpleType>
                            <xs:restriction base="xs:string">
                                <xs:maxLength value="1000" />
